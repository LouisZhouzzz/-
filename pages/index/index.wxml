<view class='header' style='height:{{h30}}px'>
  <view class='circleBar'>
    <view class="wrap">
      <view class="top">
        <canvas class="cir" style=' width:106px; height:106px;' canvas-id="canvasArc"></canvas>
        <view class="centerWord">{{resultComment}}</view>
      </view>
    </view>
  </view>
</view>

<!-- longitude='{{longitude}}'
latitude='{{latitude}}' -->
<map longitude='{{longitude}}' latitude='{{latitude}}' markers='{{markers}}' show-show-location='true' show-compass='true' enable-3D='true' scale='18' style='height:{{h70}}px;width:100%;'>

  <cover-view class='getLoc'>
      <cover-image src='/image/getLoc.png' class='getLoc-img' bindtap='reGetLoc'></cover-image>
    </cover-view>
  <cover-view class='cpy'>
      <cover-image src='/image/cpr.jpg' class='btn-cpy' bindtap='callCPY'></cover-image>
  </cover-view>
</map>


